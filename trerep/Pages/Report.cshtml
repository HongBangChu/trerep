@page
@model trerep.Pages.ReportModel
@{
    ViewData["Title"] = "Report";
}
<link href="~/css/worksheet.css" rel="stylesheet" />
<!-- tabs -->
<div class="tabbable tabs-bottom">
    <div class="tab-content">
        <div class="tab-pane active" id="fxtran">
            <partial name="~/Pages/Shared/Data/_FxTranTabPartial.cshtml" />
        </div>
    </div>
    <!-- tab content -->
    <ul class="nav nav-tabs">
        <li class="nav-item dropup">
            <a href="#fxtran" data-toggle="tab" class="nav-link active show dropbtn">FX Tran</a>
            <div class="dropup-content">
                <a id="save-fxtran" href="#">Save</a>
            </div>
        </li>
        <li class="nav-item"><a class="nav-link" href="#services" data-toggle="tab">Services</a></li>
        <li id="pager"></li>
    </ul>

</div>
<!-- /tabs -->
<div class="modal" id="result-modal">
    <div class="modal-dialog">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title"></h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                Modal body..
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>

        </div>
    </div>
</div>
<script type="text/javascript">
    var showResult = function (title, content) {
        $('#result-modal .modal-title').text(title);
        $('#result-modal .modal-body').html(content);
        $('#result-modal').modal('show');
    }
</script>
<form method="post" enctype="multipart/form-data">
    <input />
    <button asp-page-handler="Export">Export</button>
</form>
